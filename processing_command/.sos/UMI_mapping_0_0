-S ../data/Data/20170120_second/323T_hmC_jump_cfDNA.umi_encoded_adaptor_removed_no_mismatch_sorted_dedup.bam -L ../data/Data/20170120_second/323T_hmC_jump_cfDNA.UMI_dedup_no_mismatch.log
